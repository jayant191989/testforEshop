@model HR_Management.Models.Batch


<div class="modal-header">
    <a class="close" data-dismiss="modal" aria-hidden="True">&times;</a>
    <h4 class="modal-title">Are you sure you want to delete this?</h4>
</div>

<div class="modal-body">
    <p id="MessageToClient" class="text-danger"></p>
  
    <div>

        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.EnteredDate)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.EnteredDate)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.IsActive)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.IsActive)
            </dd>

        </dl>


    </div>
</div>

@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "deletebatchform", @class = "deletebatchform" }))
{
    @Html.AntiForgeryToken()

    <div class="modal-footer">
        <a class="btn btn-default" data-dismiss="modal">Cancel</a>
        <input class="btn btn-primary" type="submit" value="Yes" />
    </div>
}






